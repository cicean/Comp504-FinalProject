package common.message.chat;

import common.message.ARequest;
import common.message.IChatMessage;

/**
 * A message asking for a command for an unknown data type. 
 */
public abstract class ACommandRequest extends ARequest implements IChatMessage {

	/**
	 * Auto-generated UID. 
	 */
	private static final long serialVersionUID = -8514054368433539853L;

	/**
	 * Returns the class type of unknown data. 
	 * @return the class type of unknown data. 
	 */
	public abstract Class<?> getUnknownType();	

}
